{
    "name": "teamtnt/php-stripe-webhook-tester",
    "description": "A PHP package for testing Stripe Webhooks localy",
    "keywords": [
        "teamtnt",
        "php-stripe-webhook-tester"
    ],
    "homepage": "https://github.com/teamtnt/php-stripe-webhook-tester",
    "license": "MIT",
    "authors": [
        {
            "name": "Nenad Ticaric",
            "email": "nticaric@gmail.com",
            "homepage": "http://www.tntstudio.us",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.4.0",
        "nesbot/carbon": "~1.0",
        "guzzlehttp/guzzle": "~5.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "TeamTNT\\Stripe\\Webhook\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TeamTNT\\Stripe\\Webhook\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
